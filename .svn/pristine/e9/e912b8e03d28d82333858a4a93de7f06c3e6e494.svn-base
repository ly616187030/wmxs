<extend name="./Application/Admin/View/Public/layout.html"/><block name="style">    <style>        body{            padding:20px;            font-family: "Microsoft Yahei";        }    </style></block><block name="main">    <div class="cf form-group form-horizontal">        <form id="search_form" onsubmit="return false;">            <label class="control-label col-sm-1">请选择餐厅</label>            <div class="col-sm-2">                <select class="form-control" name="restaurant">                    <volist name="_store" id="vo" key="k">                        <option value="{$vo.store_id}" <eq name="k" value="1">selected="selected"</eq> >{$vo.store_name}</option>                    </volist>                </select>            </div>            <div class="col-sm-3">                <label class="control-label col-sm-4">起始时间</label>                <div class="col-sm-7">                    <input type="text" name="stime" class="form-control date" value=""/>                </div>            </div>            <div class="col-sm-3">                <label class="control-label col-sm-4">结束时间</label>                <div class="col-sm-7">                    <input type="text" name="etime" class="form-control date" value=""/>                </div>            </div>            <div class="2">                <button type="button"  class="btn btn-success" id="search"><span class="glyphicon glyphicon-search"></span>查询</button>            </div>        </form>    </div>    <div class="data-table table-striped m-b">        <form class="ids">            <table class="table">                <thead>                <tr>                    <th class="row-selected">                        <input class="checkbox check-all" type="checkbox">                    </th>                    <th>成功订单</th>                    <th>取消订单</th>                    <th>退款订单</th>                    <th>商家据单</th>                    <th>完成时间</th>                </tr>                </thead>                <tbody>                <notempty name="msg">                    <volist name="msg" id="vo">                        <tr>                            <td>                            </td>                            <td>共{$vo.c}单 ￥{$vo.cm}</td>                            <td>共{$vo.q}单 ￥{$vo.qm}</td>                            <td>共{$vo.t}单 ￥{$vo.tm}</td>                            <td>共{$vo.j}单 ￥{$vo.jm}</td>                            <td>{$vo.time}</td>                        </tr>                    </volist>                    <else/>                    <td colspan="7" class="text-center"> aOh! 暂时还没有内容!</td>                </notempty>                </tbody>            </table>        </form>        <div class="page">{$page}</div>    </div></block><block name="script">    <script type="text/javascript" src="__PUBLIC__/angular-1.4.js"></script>    <script type="text/javascript" src="__PUBLIC__/datetimepicker/js/bootstrap-datetimepicker.min.js"></script>    <script type="text/javascript" src="__PUBLIC__/datetimepicker/js/locales/bootstrap-datetimepicker.zh-CN.js" charset="UTF-8"></script>    <script type="text/javascript">        $(document).ready(function(){            $('.date').datetimepicker({                format: 'yyyy-mm-dd',                language:"zh-CN",                minView:2,                autoclose:true            });            $('#search').click(function() {                var query=$('#search_form').serialize();                $.get("{:U('Statistics/completion')}",query,function(data){                    var msg = '';                    for(var i=0; i<data.length;i++){                        msg +='<tr> <td> </td> <td>共'+data[i]['c']+'单 ￥'+data[i]['cm']+'</td> <td>共'+data[i]['q']+'单 ￥'+data[i]['qm']+'</td> <td>共'+data[i]['t']+'单 ￥'+data[i]['tm']+'</td> <td>共'+data[i]['j']+'单 ￥'+data[i]['jm']+'</td><td>'+data[i]['time']+'</td> </tr>'                    }                    $('tbody').html(msg);                });            });        });    </script></block>