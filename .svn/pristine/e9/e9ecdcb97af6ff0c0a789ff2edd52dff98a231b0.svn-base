<extend name="./Application/Admin/View/Public/layout.html"/><block name="main">	<div class="row">		<div class="col-sm-2" style="margin-top: 20px;margin-left:50px;width: auto;padding: 6px 35px 6px 35px;font-size:16px;">配送范围</div>	</div>	<div class="data-table table-striped" style="margin-top: 20px;padding:0px 15px 0px 15px ;">	<form class="ids">		<table class="table-bordered table text-center">			<thead>				<tr>					<th>商家ID</th>					<th>商家店铺名称</th>					<th>所属商圈</th>                    <th>配送坐标</th>					<th>操作</th>				</tr>			</thead>			<tbody>				<notempty name="list">				<volist name="list" id="s1">					<tr>						<td>{$s1.store_id}</td>						<td>{$s1.store_name}</td>                        <td>{$s1.sq_name}</td>						<td>                            <if condition="($s1.distribution_range eq null)">                                未设置                                <else />                                {$s1.distribution_range|substr=0,40}...                            </if>                        </td>						<td>                            <a href="{:U('del?store_id='.$s1['store_id'])}" class="ajax-get" >清除已设置坐标</a>                            <if condition="($s1.distribution_range eq null)">                                <a title="设置配送范围" href="{:U('edit?store_id='.$s1['store_id'])}">设置配送范围</a>                                <else />                                <a title="修改配送范围" href="{:U('edit?store_id='.$s1['store_id'])}">修改配送范围</a>                            </if>						</td>					</tr>				</volist>				<else/>				<td colspan="9" class="text-center"> aOh! 暂时还没有内容! </td>				</notempty>			</tbody>		</table>	 </form>		<!-- 分页 -->	    <div class="page">	        {$page}	    </div>	</div>	<block name="script"><script type="text/javascript" src="__STATIC__/uploadify/jquery.uploadify.min.js"></script><script type="text/javascript"></script>	</block>