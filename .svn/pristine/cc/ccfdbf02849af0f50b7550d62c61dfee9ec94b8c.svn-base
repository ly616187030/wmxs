<extend name="./Application/Admin/View/Public/layout.html"/><block name="style">    <style>        body{            padding:20px;            font-family: "Microsoft Yahei";        }    </style></block><block name="main">    <div class="main-title m-b">    </div>    <!-------查询按钮--------->    <notempty name="store_id">        <div class="form-group" style="border-bottom: 1px solid #DDDCDC;border-top: 1px solid #DDDCDC;">        <div class="form-inline"  style="padding: 20px 0px 20px 0px;font-size: 14px;">            <div style="width:100%;margin-left:10px;">                <div class="form-group"  style="width: 100%;margin:0px;padding:0px;">                    <label  for="exampleInputEmail3">下单时间：</label>                    <input type="text" readonly="readonly" class="form-control date" style="width:12.5%;"  id="exampleInputEmail3" name="stime" placeholder="请选择日期" <notempty name="stime">value="{$stime|date='Y-m-d',###}"</notempty> >                    <i class="fa fa-repeat" style="color: #0edd81;cursor: pointer;margin-left: -30px;"></i>&nbsp;&nbsp;                    <label  for="exampleInputEmail3" style="margin-left:15px;">至</label>                    <input type="text"  readonly="readonly"  class="form-control date" style="width:12.5%;margin-left:15px;" id="exampleInputEmail4" name="etime"  placeholder="请选择日期" <notempty name="etime">value="{$etime|date='Y-m-d',###}"</notempty> >                    <i class="fa fa-repeat" style="color: #0edd81;cursor: pointer;margin-left: -30px;"></i>                    <label  for="exampleInputEmail3" style="margin-left:30px;">关键字：</label>                    <input type="text" class="form-control" id="order_sn" value="{$order_sn}" name="order_sn" placeholder="订单编号" style="width:14%;">                    <label  for="exampleInputEmail3" style="margin-left:30px;">商家名称：</label>                    <select class="form-control" name="storename" id="store_id" style="width:16%">                        <option value="">全部</option>                        <volist name="store" id="vo">                            <option value="{$vo.store_id}" <eq name="store_id" value="$vo.store_id">selected="true"</eq>>{$vo.store_name}</option>                        </volist>                    </select>                    <button class="btn btn-success btn-cha ajax-get" type="button" style="margin-left: 20px;">查询</button>                </div>                <br/>                <style>                    .menu_status{ width: 350px; float: left;}                    .menu_status2{width: 490px; float: left;}                </style>                <div class="form-group" style="margin-top:15px; width: 100%;">                    <div class="menu_status">                        <label  for="exampleInputEmail3">订单状态：</label>                        <input type="radio" class="radio status" value="" name="status" id="status"  <eq name="status" value="">checked="checked"</eq>><label style="font-size: 13px;font-weight: 100"  for="status">全部</label>                        <input type="radio" class="radio status" value="4" name="status"  id="status1" <eq name="status" value="4">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="status1">已确认</label>                        <input type="radio" class="radio status" value="5" name="status"   id="status2"<eq name="status" value="5">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="status2">已完成</label>                        <input type="radio" class="radio status" value="10" name="status"  id="status3" <eq name="status" value="10">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="status3">无效</label>                    </div>                    <div class="menu_status">                        <label  for="exampleInputEmail3">支付类型：</label>                        <input type="radio" class="radio pay_type" value="" name="pay_type" id="pay_type"  <eq name="pay_type" value="">checked="checked"</eq>><label style="font-size: 13px;font-weight: 100" for="pay_type">全部</label>                        <input type="radio" class="radio pay_type" value="1" style="margin-left: 10px;" name="pay_type"  id="pay_type1" <eq name="pay_type" value="1">checked="checked"</eq>><label style="font-size: 13px;font-weight: 100" for="pay_type1">在线支付</label>                        <input type="radio" class="radio pay_type" value="0" style="margin-left: 10px;" name="pay_type"  id="pay_type2" <eq name="pay_type" value="0">checked="checked"</eq>><label style="font-size: 13px;font-weight: 100" for="pay_type2">线下支付</label>                    </div>                    <div class="menu_status2">                        <label  for="exampleInputEmail3">接单后时间段筛选：</label>                        <input type="radio" class="radio confirm_time" value="" name="confirm_time" id="period" <eq name="period" value="">checked="checked"</eq>><label style="font-size: 13px;font-weight: 100" for="send_time">全部</label>                        <input type="radio" class="radio confirm_time" value="1" name="confirm_time" id="period1" <eq name="period" value="1">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="period1">>30分钟</label>                        <input type="radio" class="radio confirm_time" value="2" name="confirm_time"  id="period2"<eq name="period" value="2">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="period2">>40分钟</label>                        <input type="radio" class="radio confirm_time" value="3" name="confirm_time" id="period3" <eq name="period" value="3">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="period1">>50分钟</label>                        <input type="radio" class="radio confirm_time" value="4" name="confirm_time"  id="period4"<eq name="period" value="4">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="period2">>60分钟</label>                        <!--<label  for="exampleInputEmail3">订单类型：</label>                        <input type="radio" class="radio send_time" value="" name="send_time" id="send_time" <eq name="send_time" value="">checked="checked"</eq>><label style="font-size: 13px;font-weight: 100" for="send_time">全部</label>                        <input type="radio" class="radio send_time" value="1" name="send_time" id="send_time1" <eq name="send_time" value="1">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="send_time1">预约单</label>                        <input type="radio" class="radio send_time" value="2" name="send_time"  id="send_time2"<eq name="send_time" value="2">checked="checked"</eq> style="margin-left: 10px;"><label style="font-size: 13px;font-weight: 100" for="send_time2">即时单</label>-->                    </div>                </div>                <br/>            </div>        </div>    </div>    <!-------查询按钮结束--------->    <style type="text/css">        .span{            float: left;            margin-left:40px;        }        table tbody tr td{            text-align: center;        }    </style>    <div style="padding:10px">共有&nbsp;&nbsp;<a class="shuliang" style="color: red;">{$count}</a>&nbsp;&nbsp;条订单记录   &nbsp;&nbsp;订单金额&nbsp;&nbsp;<a class="sumt" style="color: red;">{$sum+$ps_cost+$canju_total-$yh_price}</a>&nbsp;&nbsp;元</div>    <div class="order_id">        <div class="like">            <notempty name="list">                <volist name="list" id="data">                    <!-----输出菜品------>                    <div class="cf" id="order_{$data.order_id}" style="border: 1px solid #dddddd;margin-bottom: 20px;height: auto;">                        <div style="width: 100%;height:43px;background-color:#f3f3f3;border-bottom: 1px solid #dddddd">                            <span class="span" style="line-height: 43px;color:#666">#{$data.order_count}</span>                            <span class="span" style="line-height: 43px;">订单编号：{$data.order_sn}</span>                            <span class="span" style="line-height: 43px;">商家名称：{$data.store_name}</span>                            <span class="span" style="line-height: 43px;">下单时间：{$data.xd_time|date='Y-m-d  H:i',###}</span>                            <span class="span" style="line-height: 43px;">订单金额：￥{$data['total']+$data['ps_cost']+$data['canju_total']-$data['yh_price']}</span>                            <span style="line-height: 43px;float: right;margin-right: 80px;">订单状态：                                <switch name="data.status">                                    <case value="1">未支付订单</case>                                    <case value="2">已支付订单</case>                                    <case value="3">货到付款</case>                                    <case value="4">商家已确认</case>                                    <case value="5">完成订单</case>                                    <case value="6">完成评价</case>                                    <case value="7">取消订单</case>                                    <case value="8">取消已付款订单</case>                                    <case value="9">商家拒单 &nbsp;&nbsp;&nbsp;&nbsp;原因：{$data.reject_reason}</case>                                    <case value="10">商家拒单 &nbsp;&nbsp;&nbsp;&nbsp;原因：{$data.reject_reason}</case>                                    <case value="11">退款中</case>                                    <case value="12">退款完成</case>                                    <case value="13">用户下单，商家接单，后用户取消订单</case>                                    <case value="14">制作完成</case>                                    <case value="15">退款中</case>                                    <case value="16">退款中</case>                                    <case value="17">退款完成</case>                                    <case value="18">退款完成</case>                                    <default />                                </switch>                            </span>                        </div>                        <div style="margin-top: 15px;padding:0px 30px 5px 30px;float:left;width:50%;">                            <eq name="is_print" value="1"><button type="submit" class="btn btn-primary daying" data-id="{$data.order_id}">前台小票打印</button></eq>                            <if condition="$data.status eq 9">                                <button type="submit" class="btn btn-warning" disabled="disabled"  style=" margin-left:25px;">  拒单完成  </button>                                <elseif condition="$data.status eq 10"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  拒单完成  </button>                                <elseif condition="$data.status eq 5"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  已完成订单  </button>                                <elseif condition="$data.status eq 7"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  取消订单  </button>                                <elseif condition="$data.status eq 8"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  取消已付款订单  </button>                                <elseif condition="$data.status eq 11"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  退款中  </button>                                <elseif condition="$data.status eq 12"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  退款完成  </button>                                <elseif condition="$data.status eq 13"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  用户取消订单  </button>                                <elseif condition="$data.status eq 15"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  退款中  </button>                                <elseif condition="$data.status eq 16"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  退款中  </button>                                <elseif condition="$data.status eq 17"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  退款完成  </button>                                <elseif condition="$data.status eq 18"/>                                <button type="submit" class="btn btn-warning" disabled="disabled"   style=" margin-left:25px;">  退款完成  </button>                                <else/>                                <button type="submit" class="btn btn-warning" data-id="{$data.order_id}" data-status="{$data.status}" data-toggle="modal" data-target="#myModal" style=" margin-left:25px;">  拒单  </button>                            </if>                            <if condition="$data.status eq 5">                                <button type="submit" class="btn btn-success" disabled="disabled" style=" margin-left:25px;">已完成</button>                                <elseif condition="$data.status eq 9"/>                                <button type="submit" class="btn btn-success" disabled="disabled" style=" margin-left:25px;">已拒单</button>                                <elseif condition="$data.status eq 10"/>                                <button type="submit" class="btn btn-success" disabled="disabled" style=" margin-left:25px;">已拒单</button>                                <elseif condition="$data.status eq 4"/>                                <button type="submit" class="btn btn-success wancheng" data-id="{$data.order_id}" style=" margin-left:25px;">完成</button>                                <elseif condition="$data.status eq 6"/>                                <button type="submit" class="btn btn-success wancheng" data-id="{$data.order_id}" style=" margin-left:25px;">完成</button>                                <elseif condition="$data.status eq 14"/>                                <button type="submit" class="btn btn-success wancheng" data-id="{$data.order_id}" style=" margin-left:25px;">完成</button>                                <else/>                                <button type="submit"  disabled="disabled"  class="btn btn-success wancheng" style=" margin-left:25px;">完成</button>                            </if>                            <if condition="$data.status eq 9">                                <button type="submit" class="btn btn-default" disabled="disabled" style=" margin-left:25px;"> 已拒单  </button>                                <elseif condition="$data.status eq 10"/>                                <button type="submit" class="btn btn-default" disabled="disabled" style=" margin-left:25px;"> 已拒单 </button>                                <elseif condition="$data.status eq 5"/>                                <button type="submit" class="btn btn-default" disabled="disabled" style=" margin-left:25px;"> 已完成 </button>                            </if>                            <button class="btn btn-danger delete" style="margin-left:25px;" data-id="{$data.order_id}">删 除</button>                            <p style="margin-top:20px;">顾客ID：{$data.uid}                     <span style="margin-left: 40px;">                        <if condition="$data.gender eq 1">                            顾客姓名：{$data.username}先生                            <else/>                            顾客姓名：{$data.username}女士                        </if>                     </span>                                <span style="margin-left: 40px;">顾客电话：{$data.phone}</span>                                <span style="margin-left: 40px;">顾客地址：{$data.detail_address}</span>                            </p>                            <p style="margin-top:20px;">订单备注：{$data.order_message}</p>                            <p style="margin-top:20px;">期望时间：<empty name="data.send_time">立即送达<else />{$data.send_time|date="Y-m-d H:i",###}</empty></p>                            <!--<p style="margin-top:20px;">订单来源：<neq name="data.from_operation" value="4">网络订单<else />快速下单-{$data.source|getSource}</neq></p>-->                            <p style="margin-top:20px;background-color: #89c900;border-radius: 10px;width:75px;">                <span style="padding:8px;color: #fff">                    <if condition="$data.pay_type eq 1">                        在线支付                        <else/>                        货到付款                    </if>                </span>                            </p>                        </div>                        <div style="clear: both;"></div>                        <div style="margin-top: 15px;padding:0px 30px 5px 30px;height:auto;float:left;width:95%">                            <div style="width: 55%;height: auto;border: 1px solid #dddddd;float: left;">                                <table style="line-height:30px;width: 100%;height: 30px;">                                    <thead style="background-color: #F1F1F1;border-bottom: 1px solid #dddddd">                                    <th>菜品</th>                                    <th>单价</th>                                    <th>数量</th>                                    <th>金额</th>                                    </thead>                                    <tbody>                                    <volist name="data['child']" id="vo">                                        <tr>                                            <td>{$vo['cm_name']}</td>                                            <td>{$vo['price']}</td>                                            <td>{$vo['quantity']}</td>                                            <td>{$vo['total_money']}</td>                                        </tr>                                    </volist>                                    <tr style="border-top: 1px solid #dddddd;background-color:#F1F1F1; ">                                        <td>小计：</td>                                        <td></td>                                        <td></td>                                        <td >{$data.total}</td>                                    </tr>                                    </tbody>                                </table>                            </div>                            <div style="width: 34%;border: 1px solid #dddddd;height:auto;float:right">                                <table style="line-height:30px;width: 100%;">                                    <thead style="background-color: #F1F1F1;border-bottom: 1px solid #dddddd;">                                    <th>其它费用</th>                                    <th>金额</th>                                    </thead>                                    <tbody>                                    <tr>                                        <td>配送费</td>                                        <td>{$data.ps_cost}</td>                                    </tr>                                    <tr>                                        <td>餐盒费</td>                                        <td>{$data.canju_total}</td>                                    </tr>                                    <tr>                                        <td>优惠</td>                                        <td>{$data.yh_price}</td>                                    </tr>                                    </tbody>                                </table>                            </div>                        </div>                        <div style="clear:both "></div>                        <div style="height: 40px;margin-right:10%;float: none;margin-top:50px;">                            <p style="float: right">&nbsp;&nbsp;总计：<span style="color: red;">{$data['total']+$data['ps_cost']+$data['canju_total']-$data['yh_price']}元</span></p>                        </div>                        <!-- <div style="height: 50px;margin-top: 20px;border-top: 1px solid #dddddd;float: none;">                             <p style="line-height: 50px;margin-left: 40px;">第三方配送电话：1546461151656</p>                         </div>-->                    </div>                    <!-----输出菜品结束------>                    <!--顾客小票start-->                    <div class="ticketMain" id="ticket_{$data.order_id}" style="display:none;">                        <div style="font-size: 30px;font-weight: bold">外卖-#{$data.order_count}</div>                        <div class="ticketMainTitle">                            给顾客专用                        </div>                        <div class="Blank10">*****************************</div>                        <div class="Title fw"><div class="pdt5" id="customer_ticket_pay_type"><if condition="$data.pay_type eq 0">[货到付款]<else/>[在线支付]</if></div></div>                        <div class="Blank10">*****************************</div>                        <div class="pdt5" id="customer_ticket_send_time"> 期望送达时间：<empty name="data.send_time">立即送达<else />{$data.send_time|date="Y-m-d H:i",###}</empty></div><div class="pdt5" id="customer_ticket_order_message">订单备注：{$data.order_message}</div>                        <div class="Blank10">*****************************</div>                        <div class="pdt5" id="customer_ticket_order_sn"> 订单编号：{$data.order_sn}</div><div class="pdt5" id="customer_ticket_xd_time">下单时间：{$data.xd_time|date="Y-m-d H:i:s",###}</div>                        <div class="Blank10">*****************************</div>                        <table class="fz18">                            <thead><th align="left">菜单名称</th><th align="center">数量</th><th align="right">金额</th></thead>                            <tbody id="good_detail">                            <volist name="data.child" id="vo">                                <tr>                                    <td width="60%" align="left">{$vo['cm_name']}</td>                                    <td width="20%" align="center">{$vo['quantity']}</td>                                    <td width="20%" align="right">{$vo['total_money']}</td>                                </tr>                            </volist>                            <tr>                                <td width="80%" colspan="2" align="left">餐盒费：</td>                                <td width="20%" align="right">{$data.canju_total}</td>                            </tr>                            <!---->                            <tr>                                <td width="80%" colspan="2" align="left">配送费：</td>                                <td width="20%" align="right">{$data.ps_cost}</td>                            </tr>                            <tr>                                <td width="80%" colspan="2" align="left">优惠：</td>                                <td width="20%" align="right">{$data.yh_price}</td>                            </tr>                            <tr>                                <td width="80%" colspan="2" align="left">总计：</td>                                <td width="20%" align="right">{$data['total']+$data['ps_cost']+$data['canju_total']-$data['yh_price']}</td>                            </tr>                            </tbody>                        </table>                        <div class="Blank10">*****************************</div>                        <div class="pdt5 fz18 fw" id="customer_ticket_username">姓名：{$data['username']}&nbsp;&nbsp;&nbsp;<eq name="data.gender" value="1">先生<else />女士</eq></div>                        <!---->                        <div class="pdt5 fz18 fw" id="customer_ticket_detail_address"> 地址：{$data['detail_address']}</div>                        <div class="pdt5 fz18 fw" id="customer_ticket_phone"> 电话：{$data['phone']}</div>                        <div class="Blank10">*****************************</div>                        <div class="pdt5" id="customer_ticket_store_name"> {$data['store_name']}</div>                        <div class="pdt5" id="customer_ticket_tel"> {$data['tel']}</div>                        <div class="Blank10">*****************************</div>                        <div class="pdt5" id="customer_ticket_now_time" style="font-size: 10px"> <em>由外卖先生提供技术支持 {$time|date="Y年m月d日 H:i:s",###}</em></div>                        <div class="Blank10">*****************************</div>                    </div>                    <!--顾客小票end-->                </volist>                <else/>                <div style="height: 50px;margin-top: 20px;border: 1px solid #dddddd;background-color: #f3f3f3"> <p style="line-height: 50px;text-align: center;">ON 今天暂时还没有订单</p> </div>            </notempty>             </div>        </div>        <div class="page">{$page}</div>    </div>        <else />        <div style="height: 50px;margin-top: 20px;border: 1px solid #dddddd;background-color: #f3f3f3"> <p style="line-height: 50px;text-align: center;">当前帐号没有分配店铺</p> </div>    </notempty>    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">        <div class="modal-dialog">            <div class="modal-content">                <div class="modal-header">                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>                    <h4 class="modal-title">请输入拒单原因</h4>                </div>                <div class="modal-body">                    <div class="jinggao" style="width: 100%;height: 30px;background-color:#F1F1F1;border: 1px solid #F2F2F2;display: none"><p style="line-height: 30px;color: red;">拒单原因不能为空！！！</p></div>                    <textarea name="reject_reason" cols="73" rows="5" placeholder="请输入拒单详情. . ." style="margin-top: 20px;"></textarea>                </div>                <div class="modal-footer">                    <button type="button" class="btn btn-info" data-dismiss="modal">取消</button>                    <button type="button" class="btn btn-primary confirm">确认</button>                </div>            </div><!-- /.modal-content -->        </div><!-- /.modal-dialog -->    </div><!-- /.modal --></block><block name="script">    <script type="text/javascript" src="__PUBLIC__/datetimepicker/js/bootstrap-datetimepicker.min.js"></script>    <script type="text/javascript" src="__PUBLIC__/datetimepicker/js/locales/bootstrap-datetimepicker.zh-CN.js" charset="UTF-8"></script>    <eq name="is_print" value="1"><script type="text/javascript" src="__PUBLIC__/LodopFuncs.js"></script></eq>    <script type="text/javascript">        var is_print = "{$is_print}";        if(is_print == 1){            var LODOP=getLodop();        }        $(document).ready(function(){            $('[data-toggle="popover"]').popover();            $('.date').datetimepicker({                format: 'yyyy-mm-dd',                language:"zh-CN",                minView:2,                autoclose:true            });            function dateTime(){                $.each($('.time'),function(){                    var order_id=$(this).val();                    var status=$(this).data('status');                    var ps_status=$(this).data('ps_status');                    var source=$(this).data('source');                    var from=$(this).data('from');                    var type=$(this).data('type');                    var shengyu_time=$(this).data('shengyu_time');                    var dingge_time=$(this).data('dingge_time');                    if(from==4 && source=='电话订单'){                        if(status==4 && ps_status!=4){                            var shengyuAbs = Math.abs(shengyu_time);                            var  minute=0;                            minute = Math.floor(shengyuAbs / 60);                            var div = document.getElementById('timer_'+order_id);                            if(shengyu_time >= 0){                                div.innerHTML = '<span class="minutes" style="color:blue;">'+minute+'分</span>';                            }else{                                div.innerHTML = '<span class="minutes" style="color:blue;">-'+minute+'分</span>';                            }                        }                        if(status==4 && ps_status==4){                            var dinggeAbs = Math.abs(dingge_time);                            var  minute=0;                            minute = Math.floor(dinggeAbs / 60);                            var div = document.getElementById('timer_'+order_id);                            if(dingge_time >= 0){                                div.innerHTML = '<span class="minutes" style="color:blue;">'+minute+'分</span>';                            }else{                                div.innerHTML = '<span class="minutes" style="color:blue;">-'+minute+'分</span>';                            }                        }                    }                    if(from==4 && source==1){                        if(status==4 && ps_status!=4){                            var shengyuAbs = Math.abs(shengyu_time);                            var  minute=0;                            minute = Math.floor(shengyuAbs / 60);                            var div = document.getElementById('timer_'+order_id);                            if(shengyu_time >= 0){                                div.innerHTML = '<span class="minutes" style="color:blue;">'+minute+'分</span>';                            }else{                                div.innerHTML = '<span class="minutes" style="color:blue;">-'+minute+'分</span>';                            }                        }                        if(status==4 && ps_status==4){                            var dinggeAbs = Math.abs(dingge_time);                            var  minute=0;                            minute = Math.floor(dinggeAbs / 60);                            var div = document.getElementById('timer_'+order_id);                            if(dingge_time >= 0){                                div.innerHTML = '<span class="minutes" style="color:blue;">'+minute+'分</span>';                            }else{                                div.innerHTML = '<span class="minutes" style="color:blue;">-'+minute+'分</span>';                            }                        }                    }                    if( from!=4 && (type==0 || type==1)){                        if(status==4 && ps_status!=4){                            var shengyuAbs = Math.abs(shengyu_time);                            var  minute=0;                            minute = Math.floor(shengyuAbs / 60);                            var div = document.getElementById('timer_'+order_id);                            if(shengyu_time >= 0){                                div.innerHTML = '<span class="minutes" style="color:blue;">'+minute+'分</span>';                            }else{                                div.innerHTML = '<span class="minutes" style="color:blue;">-'+minute+'分</span>';                            }                        }                        if(status==4 && ps_status==4){                            var dinggeAbs = Math.abs(dingge_time);                            var  minute=0;                            minute = Math.floor(dinggeAbs / 60);                            var div = document.getElementById('timer_'+order_id);                            if(dingge_time >= 0){                                div.innerHTML = '<span class="minutes" style="color:blue;">'+minute+'分</span>';                            }else{                                div.innerHTML = '<span class="minutes" style="color:blue;">-'+minute+'分</span>';                            }                        }                    }                });            }            dateTime();            function Trans_php_time_to_str(timestamp,n){                var update = new Date(timestamp*1000);                var year   = update.getFullYear();                var month  = (update.getMonth()+1<10)?('0'+(update.getMonth()+1)):(update.getMonth()+1);                var day    = (update.getDate()<10)?('0'+update.getDate()):(update.getDate());                var hour   = (update.getHours()<10)?('0'+update.getHours()):(update.getHours());                var minute = (update.getMinutes()<10)?('0'+update.getMinutes()):(update.getMinutes());                var second = (update.getSeconds()<10)?('0'+update.getSeconds()):(update.getSeconds());                if(n==1){                    return (year+'-'+month+'-'+day+' '+hour+':'+minute);                }else if(n==2){                    return (year+'-'+month+'-'+day);                }else{                    return 0;                }            }            $('.btn-cha,input[type="radio"]').on('click',function(){                var stime = $('#exampleInputEmail3').val();                var etime = $('#exampleInputEmail4').val();                var order_sn = $("#order_sn").val();                var status = $("input[name='status']:checked").val();                var pay_type = $("input[name='pay_type']:checked").val();                var send_time = $("input[name='send_time']:checked").val();                var store_id = $('select[name="storename"]').val();                var confirm_time = $('input[name="confirm_time"]:checked').val();                var url = "{:U('Receiveorder/Order/refuse/stime/"+stime+"/etime/"+etime+"/order_sn/"+order_sn+"/status/"+status+"/pay_type/"+pay_type+"/send_time/"+send_time+"/store_id/"+store_id+"/confirm_time/"+confirm_time+"')}";                window.location.href = url;            });            $(document).on('click','.wancheng',function(){                var id = $(this).data('id');                var confir=confirm("是否完成");                if (confir == true) {                    $.post("{:U('complete')}",{id:id},function(data){                        if(data.status == 1){                            $.alertMessager(data.wancheng,'success');                        }                    });                    setTimeout(function(){                        window.location.reload();                    },1500)                }            });            $(document).on('click','.btn-warning',function(){                status2 = $(this).data('status');                $('textarea[name="reject_reason"] ').val("");                judanid = $(this).data('id');            });            $('.confirm').click(function(){                var reject_reason = $('textarea[name="reject_reason"] ').val();                if(reject_reason != ""){                    var confir=confirm("是否拒单");                    if (confir == true) {                        $('.jinggao').hide();                        $.post('{:U("confirm1")}', {                            id: judanid,                            reject_reason: reject_reason,                            status: status2                        }, function (data) {                            if (data.status == 1) {                                $.alertMessager(data.judan, 'success');                                window.location.reload();                            } else {                                $.alertMessager(data.judan);                            }                        })                    }                }else{                    $('.jinggao').show();                }            });            $('.fa-repeat').click(function(){                $(this).prev(".date").val("");            });        });        function printing(html,printsn,num){            var num=num || 1;            var printsn= printsn || 0;            LODOP.PRINT_INIT("");            LODOP.ADD_PRINT_HTM(10,5,"100%","100%",html);            LODOP.SET_PRINTER_INDEX(printsn);            LODOP.SET_PRINT_COPIES(num);            LODOP.PRINT();        }        function print_front(){            var htmls=$('.ticketMain').html();            $.get('{:U("getLocalNum")}').success(function(data){                printing(htmls,data.printer_sn,data.nums);            });        }        $(document).on('click','.daying',function(){            var $this=$(this);            var id = $this.data('id');            var htmls = $('#ticket_'+id).html();            $.get('{:U("getLocalNum")}').success(function(data){                printing(htmls,data.printer_sn,data.nums);            });        });        $(document).on('click','.delete',function(){            var order_id = $(this).data('id');            var datatable='order';            var confir=confirm("确定要删除此项吗？");            if (confir == true) {                $.post("{:U('del')}",{order_id:order_id,datatable:datatable},function(data){                    if(data.status == 1){                        $.alertMessager(data.delete,'success');                        setTimeout(function(){                            window.location.reload();                        },2000);                    }                });            };        });    </script></block>