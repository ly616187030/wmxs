<extend name="./Application/Admin/View/Public/layout.html"/><block name="main">	<div class="row">		<div class="col-sm-2" style="margin-top: 20px;margin-left:50px;width: auto;padding: 6px 35px 6px 35px;font-size:16px;">地址范围</div>	</div>	<div class="cf"  style="margin-top: 30px;margin-left:80px;">		<div class="fl">				<a class="btn btn-info" href="{:U('add')}">新 增</a>				<button class="btn ajax-post btn-info" url="{:U('del')}" target-form="ids">删 除</button>        </div>	</div>	<div class="data-table table-striped" style="margin-top: 30px;padding:0px 15px 0px 15px ;">	<form class="ids">		<table class="table table-bordered text-center">			<thead>				<tr>					<th class="row-selected">						<input class="checkbox check-all" type="checkbox" style="margin: 0px auto">					</th>					<th>ID</th>					<th>地址范围名称</th>					<th>经度</th>					<th>纬度</th>					<th>所属商圈</th>					<th>排序</th>					<th>状态</th>					<th>操作</th>				</tr>			</thead>			<tbody>				<notempty name="list">				<volist name="list" id="_data">					<tr>						<td><input id="biaoji" class="ids row-selected" type="checkbox" name="scope_id[]" value="{$_data.scope_id}"></td>						<td>{$_data.scope_id}</td>						<td>{$_data.scope_name}</td>						<td>{$_data.lng}</td>						<td>{$_data.lat}</td>						<td>{$_data.sq_name}</td>						<td>{$_data.sort_order}</td>												 <td>                            <a href="{:U('toogleHide',array('id'=>$_data['scope_id'],'value'=>abs($_data['status']-1)))}" class="ajax-get">                            <if condition="$_data.status == 0 ">禁用    <else /> 启用</if>                            </a>                        </td>												<td>							<a title="编辑" href="{:U('edit?scope_id='.$_data['scope_id'])}">编辑</a>							<a class="confirm ajax-get" title="删除" href="{:U('del?scope_id='.$_data['scope_id'])}">删除</a>						</td>					</tr>				</volist>				<else/>				<td colspan="9" class="text-center"> aOh! 暂时还没有内容! </td>				</notempty>			</tbody>		</table>	 </form>		<!-- 分页 -->	    <div class="page">	        {$page}	    </div>	</div>	<block name="script"><script type="text/javascript" src="__STATIC__/uploadify/jquery.uploadify.min.js"></script><script type="text/javascript"></script>	</block>