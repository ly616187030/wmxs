.page{padding-left: 0; margin: 20px 0;border-radius: 4px;}
.page span{color: #fff;  background-color: #287DDE; padding: 8px 15px; border-color: #287DDE; cursor: default;}
.page span:nth-child(2){border-top-left-radius:5px;border-bottom-left-radius:5px;}
.page a{color: #287DDE;  background-color: #fff; padding: 7px 14px;border:1px solid #ccc;text-decoration: none;}
.page a:last-child{color: #287DDE;  background-color: #fff; padding: 7px 16px;border:1px solid #ccc;border-radius: 0px 5px 5px 0px;}
.page .next{margin-left:-5px;}
.page .prev{margin-right:-5px;border-radius: 5px 0px 0px 5px;}