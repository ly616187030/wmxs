                        return false;
                    }

                    if(!(/^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/.test(email.val())) && email.val()!=''){
                        $('.alert7').css('background-color','orangered');
                        $('.b_main6').show().find($('.in6')).css('border-color','transparent orangered transparent transparent');
                        $('.b_main6').show().find($('.alert7')).text('邮箱格式不正确！');
                        return false;
                    }
                }

                $.post(self.attr("action"), self.serialize(), function(data){
                    $('.tijiao').attr('disabled','disabled');
                    if(data.status){
                        $('.b_main6').show().find($('.alert7')).text(data.info);
                        $('.alert7').css('background-color','orangered');
                        $('.b_main6').show().find($('.in6')).css('border-color','transparent orangered transparent transparent');
                        setTimeout(function(){
                            window.location.href=data.url;
                        },2500);
                    }else{
                        $('.b_main6').show().find($('.alert7')).text(data.info);
                        $('.alert7').css('background-color','orangered');
                        $('.b_main6').show().find($('.in6')).css('border-color','transparent orangered transparent transparent');
                    }
                }, "json");
                return false;
            })

            //第二步结束

        });
    </script>
</block>
