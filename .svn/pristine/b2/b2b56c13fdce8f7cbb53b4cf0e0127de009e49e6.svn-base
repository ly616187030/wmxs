	$('.mui-numbox-input').keyup(function(){
		var min=this.getAttribute('data-numbox-min'),max=this.getAttribute('data-numbox-max');
		if(this.value>max){
			this.value=max;
		}
		if(this.value<min){
			this.value=min;
		}
		if(this.value.length==2){
			$(this).blur();
			$(this).nextAll('.mui-numbox-input:eq(0)').focus();
		}
