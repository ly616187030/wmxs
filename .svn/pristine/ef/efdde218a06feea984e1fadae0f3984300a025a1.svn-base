#sub_dinner_time{
			width: 72%;
			float: left;
			height: 30px;
			line-height:30px;			
			background:#fff;
			padding-left:5px;
			position:relative;
			font-size:14px;
			border:1px solid #e1e1e1;
			margin-top:8px;
		}
		.dinner_time_list {background-color:#ffffff;display:none;float:left;list-style:none outside none;position:absolute;left:-1px;top:100%;z-index:1000; width:100%;margin:0px;border:1px solid #e1e1e1;} 
		.dinner_time_list  li{
			border-bottom-width: 1px;
			border-bottom-style: solid;
			border-bottom-color: #EBEBEB;
			height:30px;
			line-height:30px;
			padding-left:5px;
		}
		.dinner_time_list  li.zt1 a{
			color: #000;	
		}
		.zt1 span{ width:50px; float:right; display:block;text-align:center;text-align:center; 	color: #0C0;}
		
		.dinner_time_list li.zt2 a{
			color: #CCC;
		}
		.zt2 span{
			width: 50px;
			float: right;
			display: block;
			color: #CCC;
			text-align:center;
		}
		
		
		.dinner_time_list  li.zt3 a{
			color: #000;	
		}
		.zt3 span{
			width: 50px;
			float: right;
			display: block;
			color: #F00;
			text-align:center;
		}
		.dinner_time_list li.zt4{
			background-color: #999;
		}
		.dinner_time_list li.zt4 a{
			color: #CCC;
		}
		.zt4 span{
			width: 50px;
			float: right;
			display: block;
			color: #CCC;
			line-height:30px;
			text-align:center;
		}
		
		.dinner_time_list  li.zt1 a,.dinner_time_list  li.zt2 a,.dinner_time_list  li.zt3 a,.dinner_time_list  li.zt4 a{ width:120px; float:left; display:block; font-size:14px; }
		.dinner_time_list > li > a {display: block;clear: both;font-weight: normal;line-height: 30px;color: #616060;white-space: nowrap;font-size: 14px;}
		
		
/*城市切换*/
.searchTime{ width:100%; height:46px; background:#fdf5e2; position:relative; }
		.dining{ height:46px; line-height:46px; width:430px; position:absolute;left:5px;}
		.dining>span{ float:left; width:20%; height:46px;font-size:14px; display:inline-block; position:relative;}
		.dining>span>img{ position:relative; vertical-align:middle; top:-2px;}
		.dining .arrowDown{display:inline-block; height:46px; line-height:46px; width:78%; font-size:14px; position:relative;}
		/*#sub_dinner_time{height:30px; line-height:30px;width:95%; padding-left:15px; margin-top:10.5px; border:1px solid #e1e1e1;}*/
		/*.arrowDown>img{ position:absolute; right:15px; top:20px;}*/
		.addrSwitch{ height:46px; line-height:46px; width:430px;  position:absolute; left:30%;}
		.addrSwitch>a{ display:inline-block;}
		.citySwitch{ width:15%; position:relative;}
		.dropdown{ width:60px;height:30px; line-height:30px; display:inline-block; padding-left:5px; background:#fff; border:0px;font-size:14px;}		.smallTrangle{position:absolute; top:-20px; left:35px;}
		.cityList{ display:none; height:auto;width:450px; padding:15px 10px; border:1px solid #e1e1e1; box-shadow:1px 3px 5px #d1d1d1; border-radius:5px; -webkit-border-radius:10px; position:absolute; top:60px; left:-20px; z-index:100; background:#fff;}
		
		.cityList>span{ height:auto; line-height:30px; display:inline-block; font-size:14px;}
		.dropdown>img{ position:absolute; right:5px; top:18px;}
		.dropdown>span{ font-size:14px;}
		.cityListT{border-bottom:1px solid #e1e1e1; padding-bottom:10px; height:auto;}
		.cityListT>span{ color:#333; font-size:14px; display:inline-block;}
		.cityListT>strong{ font-size:16px; margin-left:23px; font-weight:400;}
		.addrSwitch>p{ display:inline-block; margin-left:15px; margin-right:15px;}
		.cityListB{ height:auto;padding-top:10px; padding-bottom:15px; position:relative; }
		.cityListB>span{height:auto; display:inline-block; line-height:30px;color:#ff0000;font-size:14px;width:60px;float:left;}
		.cityListB>ul{font-size:14px;width:360px; float:left; margin-left:20px;height:auto;}
		.cityListB li{ width:60px; height:30px; line-height:30px; float:left;text-align:center; font-size:14px;}
		.cityListB li:hover{ color:#ff0000;}
		.addrSwitch .btnAddress>img{ position:relative; vertical-align:middle;}
		#sub_dinner_time>img{position:absolute; right:15px; top:10px;}
		.clear{ clear:both;}