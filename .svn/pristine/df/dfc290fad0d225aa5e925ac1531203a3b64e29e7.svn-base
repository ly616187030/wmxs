/*******css reset***********/
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;outline:0;font-family:"Microsoft Yahei","微软雅黑",Arial,Tahoma;font-size:12px;vertical-align:baseline;background:transparent;}body{line-height:1;}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block;}nav ul{list-style:none;}blockquote,q{quotes:none;}blockquote:before,blockquote:after,q:before,q:after{content:'';content:none;}a{margin:0;padding:0;font-size:100%;vertical-align:baseline;background:transparent;}ins{background-color:#ff9;color:#000;text-decoration:none;}mark{background-color:#ff9;color:#000;font-style:italic;font-weight:bold;}del{text-decoration:line-through;}abbr[title],dfn[title]{border-bottom:1px dotted;cursor:help;}table{border-collapse:collapse;border-spacing:0;}hr{display:block;height:1px;border:0;border-top:1px solid #cccccc;margin:1em 0;padding:0;}input,select{vertical-align:middle;}

/*******公用样式******/
li{list-style: none;}
.red{color: red}
a{color: #000;text-decoration: none;}
em{font-style: normal;}
.font14{font-size: 16px}
*:focus {outline: none;}
button{cursor:pointer;}

/*******右边浮动层******/
.scrollBtn {bottom: 50px;position: fixed; right: 5px; width: 32px;}
.scrollBtn ul{position: relative;}
.scrollBtn li{display: block;width: 30px;height: 30px;border: 1px solid #e1e1e1;margin-bottom: 5px;background: url("../images/zhgIndex/All-Icon.png") no-repeat;cursor: pointer;}
.scrollBtn .go-top{background-position: -63px -456px}
.scrollBtn .erweima{background-position:-16px -456px}
.scrollBtn .hidewm{width: 150px;padding: 10px;border: 1px solid #e1e1e1;position: absolute;top: -190px;left: -180px;display: none;background: #fff}

.scrollBtn .go-feedback{background:#fff}
.scrollBtn .go-feedback b{display: block;text-align: center;width: 2.5em; color: #999999;line-height: 15px}

/*指示文本仅显示一行,超出后显示省略号*/
.LineOF{overflow: hidden;white-space: nowrap;text-overflow: ellipsis;-o-text-overflow: ellipsis;}


/*******背景幕******/
.backdrop{width: 100%;height: 100%;background: #000;position: fixed;top: 0;left: 0;bottom: 0;right: 0;z-index: 100;filter:alpha(opacity=40);-moz-opacity:0.4;-khtml-opacity:0.4;opacity: 0.4;}

/*错误信息提示通用样式*/
.validation-summary-errors {
	background: url(../images/zhgIndex/1-1_16.jpg) no-repeat 5px 5px;
	background-color: #FFF2F2;
	border: 1px solid #FF8080;
	color: #333;
	line-height: 22px;
	padding: 2px 8px 2px 28px;
	margin: 0 auto;
}
.error-tips-wp {
	position: relative
}
.field-validation-error {
	color: #ff0000;
	background: url(../images/zhgIndex/er.gif) no-repeat left center;
	padding-left: 22px;
}
.field-validation-valid {
	display: none;
}
input.input-validation-error {
	border: 1px solid #ff0000;
}
input.my-input-validation-error {
	border: 1px solid #ff0000;
}

